<?xml version="1.0" encoding="UTF-8"?>
<routes
  xmlns="http://symfony.com/schema/routing"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

  <!-- Manifest file -->
  <route id="favicon_manifest" path="/manifest.json">
    <default key="_controller">AppBundle:Favicon:manifest</default>
  </route>

  <!-- Browserconfig file -->
  <route id="favicon_browserconfig" path="/browserconfig.xml">
    <default key="_controller">AppBundle:Favicon:browserConfig</default>
  </route>
</routes>
